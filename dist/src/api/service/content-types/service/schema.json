{
    "kind": "collectionType",
    "collectionName": "services",
    "info": {
        "singularName": "service",
        "pluralName": "services",
        "displayName": "Product",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "slug": {
            "type": "uid",
            "targetField": "name",
            "required": true
        },
        "description": {
            "type": "text"
        },
        "image": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images",
                "files"
            ]
        },
        "average_rating": {
            "type": "biginteger"
        },
        "reviews": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::review.review",
            "mappedBy": "service"
        },
        "category": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::category.category",
            "inversedBy": "services"
        },
        "product_status": {
            "type": "enumeration",
            "enum": [
                "Pending",
                "Approved",
                "Rejected"
            ],
            "default": "Pending"
        },
        "company": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::company.company",
            "inversedBy": "services"
        },
        "delete": {
            "type": "enumeration",
            "enum": [
                "Pending",
                "Deleted"
            ]
        },
        "targetAudience": {
            "type": "text"
        },
        "country": {
            "type": "string"
        },
        "social_accounts": {
            "type": "component",
            "repeatable": false,
            "component": "shared.social"
        },
        "url": {
            "type": "string"
        }
    }
}

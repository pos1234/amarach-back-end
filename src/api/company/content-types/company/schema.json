      "target": "api::category.category",
      "inversedBy": "companies"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "company"
    },
    "logo": {
      "allowedTypes": [
        "files",
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "featuredImages": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": true
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service",
      "mappedBy": "company"
    }
  }
